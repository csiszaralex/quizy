{
  "name": "quizy",
  "version": "0.2.0",
  "private": true,
  "description": "Egy kvíz szerkesztő oldal frontend-je",
  "author": {
    "name": "Csiszár Alex",
    "email": "csiszaralex@gmail.com",
    "url": "https://github.com/csiszaralex"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "axios": "^0.21.1",
    "bootstrap": "5.0.0-alpha2",
    "core-js": "^3.6.5",
    "firebase": "^8.2.2",
    "firebaseui": "^4.7.1",
    "popper.js": "^1.16.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuefire": "^3.0.0-alpha.2",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ],
    "plugins": []
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/attributes-order": "warn",
      "vue/attribute-hyphenation": "error",
      "vue/block-tag-newline": "error",
      "vue/component-definition-name-casing": "error",
      "vue/component-tags-order": "error",
      "vue/html-closing-bracket-newline": "warn",
      "vue/html-closing-bracket-spacing": "warn",
      "vue/html-comment-content-newline": "warn",
      "vue/html-comment-content-spacing": "warn",
      "vue/html-comment-indent": "warn",
      "vue/html-quotes": "warn",
      "vue/multiline-html-element-content-newline": "warn",
      "vue/new-line-between-multi-line-property": "off",
      "vue/no-arrow-functions-in-watch": "error",
      "vue/no-async-in-computed-properties": "error",
      "vue/no-boolean-default": "warn",
      "vue/no-confusing-v-for-v-if": "error",
      "vue/no-custom-modifiers-on-v-model": "error",
      "vue/no-deprecated-destroyed-lifecycle": "error",
      "vue/no-deprecated-html-element-is": "error",
      "vue/no-deprecated-v-bind-sync": "error",
      "vue/no-deprecated-v-on-number-modifiers": "error",
      "vue/no-deprecated-vue-config-keycodes": "error",
      "vue/no-dupe-keys": "error",
      "vue/no-dupe-v-else-if": "warn",
      "vue/no-duplicate-attributes": "error",
      "vue/no-empty-component-block": "warn",
      "vue/no-irregular-whitespace": "warn",
      "vue/no-lifecycle-after-await": "error",
      "vue/no-lone-template": "error",
      "vue/no-multi-spaces": "warn",
      "vue/no-multiple-slot-args": "warn",
      "vue/no-multiple-template-root": "off",
      "vue/no-mutating-props": "warn",
      "vue/no-parsing-error": "error",
      "vue/no-potential-component-option-typo": "warn",
      "vue/no-ref-as-operand": "error",
      "vue/no-reserved-component-names": "error",
      "vue/no-reserved-keys": "warn",
      "vue/no-restricted-component-options": "warn",
      "vue/no-setup-props-destructure": "warn",
      "vue/no-side-effects-in-computed-properties": "warn",
      "vue/no-spaces-around-equal-signs-in-attribute": "warn",
      "vue/no-template-key": "warn",
      "vue/no-template-shadow": "warn",
      "vue/no-textarea-mustache": "error",
      "vue/no-unused-components": "warn",
      "vue/no-unused-properties": "warn",
      "vue/no-unused-vars": "warn",
      "vue/no-use-v-if-with-v-for": "error",
      "vue/no-useless-mustaches": "error",
      "vue/no-useless-v-bind": "warn",
      "vue/no-v-for-template-key-on-child": "error",
      "vue/no-v-for-template-key": "off",
      "vue/no-v-html": "error",
      "vue/no-v-model-argument": "error",
      "vue/no-watch-after-await": "error",
      "vue/order-in-components": "warn",
      "vue/padding-line-between-blocks": "warn",
      "vue/prop-name-casing": "warn",
      "vue/require-component-is": "error",
      "vue/require-default-prop": "warn",
      "vue/require-direct-export": "error",
      "vue/require-name-property": "error",
      "vue/require-render-return": "error",
      "vue/require-v-for-key": "error",
      "vue/require-valid-default-prop": "error",
      "vue/return-in-computed-property": "error",
      "vue/return-in-emits-validator": "error",
      "vue/this-in-template": "error",
      "vue/use-v-on-exact": "error",
      "vue/v-bind-style": "error",
      "vue/v-on-event-hyphenation": "error",
      "vue/v-on-function-call": "error",
      "vue/v-on-style": "warn",
      "vue/v-slot-style": "warn",
      "vue/valid-v-bind-sync": "warn",
      "vue/valid-v-bind": "error",
      "vue/valid-v-cloak": "error",
      "vue/valid-v-else-if": "error",
      "vue/valid-v-else": "error",
      "vue/valid-v-for": "error",
      "vue/valid-v-if": "error",
      "vue/valid-v-is": "error",
      "vue/valid-v-model": "error",
      "vue/valid-v-on": "error",
      "vue/valid-v-once": "error",
      "vue/valid-v-pre": "error",
      "vue/valid-v-show": "error",
      "vue/valid-v-slot": "warn",
      "vue/valid-v-text": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "quotes": true,
    "printWidth": 100,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": false,
    "endOfLine": "auto",
    "semi": true,
    "requirePragma": false,
    "trailingComma": "all"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "homepage": "https://github.com/csiszaralex/quizyt#readme",
  "keywords": [
    "quizy",
    "vue"
  ],
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/csiszaralex/quizy"
  }
}
